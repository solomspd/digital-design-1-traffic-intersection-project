set_property IOSTANDARD LVCMOS33 [get_ports sensor]
set_property PACKAGE_PIN T1 [get_ports reset]
set_property IOSTANDARD LVCMOS33 [get_ports reset]
set_property PACKAGE_PIN L1 [get_ports {main_light[0]} ]
set_property IOSTANDARD LVCMOS33 [get_ports {main_light[0]} ]
set_property PACKAGE_PIN P1 [get_ports {main_light[1]} ]
set_property IOSTANDARD LVCMOS33 [get_ports {main_light[1]} ]
set_property PACKAGE_PIN N3 [get_ports {side_light[0]} ]
set_property IOSTANDARD LVCMOS33 [get_ports {side_light[0]} ]
set_property PACKAGE_PIN P3 [get_ports {side_light[1]} ]
set_property IOSTANDARD LVCMOS33 [get_ports {side_light[1]} ]
set_property PACKAGE_PIN U3 [get_ports {walk_light[0]} ]
set_property IOSTANDARD LVCMOS33 [get_ports {walk_light[0]} ]
set_property PACKAGE_PIN W3 [get_ports {walk_light[1]} ]
set_property IOSTANDARD LVCMOS33 [get_ports {walk_light[1]} ]
set_property PACKAGE_PIN W5 [get_ports clk]
set_property IOSTANDARD LVCMOS33 [get_ports clk]
create_clock -add -name sys_clk_pin -period 10.00 -waveform {0 5} [get_ports clk]


